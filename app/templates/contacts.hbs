<table class="table table-bordered">
		<tr>
			<th>Name</th>
			<th>Email</th>
			<th>Phone No#</th>
			<th>Comments</th>
			<th>Reason</th>
		</tr>
	{{#each contacts}}
		<tr>
			<td>{{fname}}</td>
			<td>{{email}}</td>
			<td>{{phoneNo}}</td>
			<td>{{comments}}</td>
			<td>{{reason}}</td>
		</tr>
	{{/each}}
</table>
